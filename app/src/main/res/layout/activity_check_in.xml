<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.blackcoffee.work.ui.activity.CheckInActivity">


    <!--标题-->
    <include
        android:id="@+id/include_title_check_in"
        layout="@layout/title_bar_aty_all_right_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />






    <!--打卡详情-->
    <android.support.v7.widget.CardView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        style="@style/CardView.Light"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="10dp"
        android:foreground="?android:attr/selectableItemBackground"
        android:stateListAnimator="@drawable/lift_on_touch"
        app:cardCornerRadius="4dp"
        app:cardElevation="4dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        tools:targetApi="lollipop"
        android:layout_below="@id/include_title_check_in"
        android:id="@+id/cv_check_info"
        >




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="10dp"
            >


            <!--头部布局-->
            <RelativeLayout
                android:id="@+id/rl_top_about"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >




                <com.blackcoffee.work.ui.widget.cirlceimageview.CircleImageView
                    android:id="@+id/civ_user_head"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:src="@drawable/test_img_head_round"
                    android:layout_centerVertical="true"

                    />


                <TextView
                    android:id="@+id/tv_user_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/black_text"
                    android:text="Slim Shady"
                    android:textStyle="bold"
                    android:layout_toRightOf="@id/civ_user_head"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"
                    />



                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:orientation="horizontal"
                    >


                    <TextView
                        android:id="@+id/tv_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/qhy_blue"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:text="2018.06.23"
                        android:textSize="13sp"
                        />



                    <TextView
                        android:id="@+id/tv_week_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/qhy_blue"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:text="Friday"
                        android:textSize="13sp"
                        android:layout_marginLeft="5dp"
                        />





                </LinearLayout>










            </RelativeLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                >



                <LinearLayout
                    android:layout_width="10dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="12dp"
                    >

                    <ImageView
                        android:id="@+id/iv_time_line_start"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/img_check_time_line_gray"

                        />

                    <View
                        android:id="@+id/v_time_line"
                        android:layout_width="0.5dp"
                        android:layout_height="match_parent"
                        android:background="@color/bg_all"
                        android:layout_weight="1"

                        />


                    <ImageView
                        android:id="@+id/iv_time_line_end"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/img_check_time_line_gray"
                        />

                </LinearLayout>


                <!--打卡布局-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    >

                    <!--上班-->
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >



                        <LinearLayout
                            android:id="@+id/ll_right"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_centerVertical="true"
                            >

                            <TextView
                                android:id="@+id/tv_morning_stipulate_time_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="mormingtime 9:00"
                                android:textSize="13sp"
                                android:textColor="@color/gray6"
                                />




                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                >
                                <TextView
                                    android:id="@+id/tv_check_time_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="mormingtime 9:00"
                                    android:textColor="@color/black_text"
                                    />

                                <TextView
                                    android:id="@+id/tv_check_status_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="3dp"
                                    android:text="access"
                                    android:layout_marginLeft="10dp"
                                    android:gravity="center"
                                    android:textSize="10sp"
                                    android:textColor="@color/white"
                                    />
                            </LinearLayout>



                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginTop="5dp"
                                android:gravity="center_vertical"
                                >

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/img_check_location"
                                    />



                                <TextView
                                    android:id="@+id/tv_location_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="mormingtime "
                                    android:textSize="13sp"
                                    android:textColor="@color/gray6"
                                    android:layout_marginLeft="5dp"
                                    />



                            </LinearLayout>




                            <LinearLayout
                                android:id="@+id/ll_update_check_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                android:layout_marginTop="5dp"
                                >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/check_in_update"
                                    android:textColor="@color/qhy_blue"
                                    android:gravity="center_vertical"
                                    android:textSize="12sp"
                                    />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/img_arrow_blue_small"
                                    android:layout_marginLeft="5dp"
                                    android:layout_gravity="center_vertical"
                                    />

                            </LinearLayout>



                        </LinearLayout>






                    </RelativeLayout>


                    <!--下班-->
                    <RelativeLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp"

                        >



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_centerVertical="true"
                            >

                            <TextView
                                android:id="@+id/tv_morning_stipulate_time_2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="afternoon 18:00"
                                android:textSize="13sp"
                                android:textColor="@color/gray6"
                                />




                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                >
                                <TextView
                                    android:id="@+id/tv_check_time_2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="afternoon 18:00"
                                    android:textColor="@color/black_text"
                                    />

                                <TextView
                                    android:id="@+id/tv_check_status_2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="3dp"
                                    android:text="access"
                                    android:layout_marginLeft="10dp"
                                    android:gravity="center"
                                    android:textSize="10sp"
                                    android:textColor="@color/white"
                                    />
                            </LinearLayout>



                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginTop="5dp"
                                android:gravity="center_vertical"
                                >

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/img_check_location"
                                    />



                                <TextView
                                    android:id="@+id/tv_location_2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="mormingtime "
                                    android:textSize="13sp"
                                    android:textColor="@color/gray6"
                                    android:layout_marginLeft="10dp"
                                    />



                            </LinearLayout>




                            <LinearLayout
                                android:id="@+id/ll_update_check_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_vertical"
                                android:layout_marginTop="5dp"
                                >

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_vertical"
                                    android:text="@string/check_in_update"
                                    android:textColor="@color/qhy_blue"
                                    android:textSize="12sp"
                                    />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/img_arrow_blue_small"
                                    android:layout_marginLeft="5dp"
                                    android:layout_gravity="center_vertical"
                                    />

                            </LinearLayout>



                        </LinearLayout>






                    </RelativeLayout>

                </LinearLayout>






            </LinearLayout>





        </LinearLayout>











    </android.support.v7.widget.CardView>



    <!--打卡按钮布局-->


    <LinearLayout
        android:id="@+id/ll_down"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@id/cv_check_info"
        android:layout_marginTop="50dp"
        android:gravity="center_vertical"
        >



        <LinearLayout
            android:id="@+id/ll_do_check_in"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_gravity="center"
            android:background="@drawable/selector_blue_circle_ring"
            >


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/check_in_only"
                android:gravity="center"
                android:textColor="@color/qhy_blue"
                android:textStyle="bold"
                android:textSize="17sp"
                />



            <Chronometer
                android:id="@+id/ch_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="00:00:00"
                android:gravity="center"
                android:format="%s"
                android:textColor="@color/qhy_blue"
                android:textSize="10sp"
                />


        </LinearLayout>




        <LinearLayout
            android:id="@+id/ll_bottom_location"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            >

            <ImageView
                android:id="@+id/iv_current_range"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:src="@drawable/img_check_mark"
                />


            <TextView
                android:id="@+id/tv_current_range"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/gray6"
                android:text="已经入打卡范围：陕西省西安市碑林区北大街99号"
                android:layout_marginLeft="10dp"
                android:textSize="12sp"
                />
        </LinearLayout>



    </LinearLayout>





</RelativeLayout>





















































